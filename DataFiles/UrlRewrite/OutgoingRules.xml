<?xml version="1.0" encoding="utf-8" ?>
<OutgoingRules>
  <Domain URL="Default">
    <CategoryRule>~/{CategoryName}-list.aspx</CategoryRule>
    <DepartmentRule>~/{DepartmentName}-items.aspx</DepartmentRule>
    <ManufacturerRule>~/{ManufacturerName}-products.aspx</ManufacturerRule>
    <ProductRule>~/{ProductName}-details.aspx</ProductRule>
    <TellFriendRule>~/TellFriend.aspx?ProductName={0}</TellFriendRule>
    <CustomerReviewRule>~/CustomerReview.aspx?ProductName={0}</CustomerReviewRule>
    <ContentRule>~/{ContentName}-content.aspx</ContentRule>
    <ContentMenuRule>~/{ContentMenuItemName}-contentlist.aspx</ContentMenuRule>
    <NewsRule>~/{NewsName}-news.aspx</NewsRule>
    <BlogRule>~/Blog/{BlogName}-blog.aspx</BlogRule>
    <BlogListRule>~/Blog/{BlogListName}-bloglist.aspx</BlogListRule>
    <BlogCategoryListRule>~/Blog/{CategoryName}-categorylist.aspx</BlogCategoryListRule>
    <PromoRule>~/{PromoName}-promo.aspx</PromoRule>
  </Domain>
